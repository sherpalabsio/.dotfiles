[
  {
    "key": "cmd+k cmd+c",
    "command": "extension.changeCase.camel",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+p",
    "command": "extension.changeCase.pascal",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+u",
    "command": "extension.changeCase.constant",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+l",
    "command": "extension.changeCase.lower",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+s",
    "command": "extension.changeCase.snake",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+backspace",
    "command": "editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+k",
    "command": "-editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "cmd+\\",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  { "key": "cmd+1", "command": "workbench.action.openEditorAtIndex1" },
  { "key": "cmd+2", "command": "workbench.action.openEditorAtIndex2" },
  { "key": "cmd+3", "command": "workbench.action.openEditorAtIndex3" },
  { "key": "cmd+4", "command": "workbench.action.openEditorAtIndex4" },
  { "key": "cmd+5", "command": "workbench.action.openEditorAtIndex5" },
  {
    "key": "shift+cmd+d",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+down",
    "command": "-editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "f1",
    "command": "editor.action.sortLinesAscending"
  },
  {
    "key": "shift+cmd+\\",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "shift+cmd+r",
    "command": "rails-go-to-spec-2.railsGoToSpec",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+y",
    "command": "-rails-go-to-spec-2.railsGoToSpec",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+m",
    "command": "extension.railsLatestMigration"
  },
  {
    "key": "cmd+k cmd+q",
    "command": "bracketeer.swapQuotes",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+; cmd+l",
    "command": "run-tests-in-iterm.runAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+; cmd+f",
    "command": "run-tests-in-iterm.runCurrentFile",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+down",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "cmd+r",
    "command": "quickOpenRails.open"
  },
  {
    "command": "toggleVim",
    "key": "ctrl+;",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+,",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.action.openSettingsJson",
        "workbench.action.openGlobalKeybindingsFile"
      ]
    }
  }
]
