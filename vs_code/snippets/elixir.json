{
  "IO.puts \"-----\"": {
    "prefix": "p",
    "body": [
      "IO.puts \"----------------------\""
    ]
  },
  "Debug variable": {
    "prefix": "pp",
    "body": [
      "IO.puts \"----------------------\"",
      "dbg(${1:params})",
      "IO.puts \"----------------------\"",
    ]
  },
  "Inspect variable": {
    "prefix": "ii",
    "body": [
      "IO.puts \"----------------------\"",
      "IO.inspect ${1:params}",
      "IO.puts \"----------------------\"",
    ]
  },
  "Inspect...": {
    "prefix": "i",
    "body": [
      "IO.inspect ",
    ]
  },
  "=> ": {
    "prefix": "=",
    "body": [
      "=> "
    ]
  },
  "->": {
    "prefix": "-",
    "body": [
      "->"
    ]
  },
  "|>": {
    "prefix": "|",
    "body": [
      "|> "
    ]
  },
  "%{}": {
    "prefix": "%",
    "body": [
      "%{$0}"
    ]
  },
  "Todo comment": {
    "prefix": "todo",
    "body": [
      "# TODO: "
    ]
  },
  "debug": {
    "prefix": "d",
    "body": [
      "require IEx",
      "IEx.pry()"
    ]
  }
}
